{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "allowCompoundWords": true,
  "numSuggestions": 0,
  "validateDirectives": true,
  "enabledFileTypes": {
    "*": true
  },
  "enableGlobDot": true,
  "useGitignore": true,
  "ignorePaths": [".git", ".gitignore", ".vscode", "*.svg", "CODEOWNERS", "drizzle", "packages/styled-system/styled-system", "naver*.html"],
  "dictionaries": ["cspell.txt"],
  "dictionaryDefinitions": [
    {
      "name": "cspell.txt",
      "path": "./cspell.txt"
    }
  ]
}
